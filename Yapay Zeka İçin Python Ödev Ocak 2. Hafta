sayi = int(input("Bir sayı girin: "))
if sayi % 2 == 0:
    print("Çift sayı")
else:
    print("Tek sayı")

notu = int(input("Notunuzu girin: "))
if 90 <= notu <= 100:
    print("Harf notu: A")
elif 80 <= notu <= 89:
    print("Harf notu: B")
elif 70 <= notu <= 79:
    print("Harf notu: C")
elif 60 <= notu <= 69:
    print("Harf notu: D")
elif 0 <= notu <= 59:
    print("Harf notu: F")
else:
    print("Geçersiz not")

yas = int(input("Yaşınızı girin: "))
if 0 <= yas <= 12:
    print("Çocuk")
elif 13 <= yas <= 19:
    print("Genç")
elif 20 <= yas <= 59:
    print("Yetişkin")
elif yas >= 60:
    print("Yaşlı")
else:
    print("Geçersiz yaş")
