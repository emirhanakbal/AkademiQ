for i in range(1, 101):
    print(i)

for i in range(2, 101, 2):
    print(i)

sayi = int(input("Bir sayı girin: "))
faktoriyel = 1
for i in range(1, sayi + 1):
    faktoriyel *= i
print("Faktöriyel:", faktoriyel)

for num in range(2, 101):
    asal = True
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            asal = False
            break
    if asal:
        print(num)
